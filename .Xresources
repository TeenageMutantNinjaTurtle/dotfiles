!Removing the scrollbar
URxvt.scrollBar: false

!the font
URxvt.font: xft:inconsolata:size=14:antialias=true
URxvt.boldFont: xft:inconsolata:size=14:antialias=true
URxvt.cursorUnderline: true
URxvt.internalBorder: 17px

!resize-font C-+, C-- and C-=
URxvt.perl-ext-common: resize-font
!URxvt.resize-font.smaller: C-j
!URxvt.resize-font.bigger: C-k

*.highlightColor: #401a63
